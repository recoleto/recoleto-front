{
  "expo": {
    "name": "Recoleto",
    "slug": "Recoleto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-w-name.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon-light.png",
      "resizeMode": "native",
      "backgroundColor": "#156F3C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-w-name.png",
        "backgroundColor": "#1D9349"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "recoleto",
    "plugins": [
     [
      "expo-font",
      {
        "fonts": ["./assets/fonts/Teachers-Regular.ttf", "./assets/fonts/Teachers-Bold.ttf", "./assets/fonts/Teachers-Medium.ttf", "./assets/fonts/Teachers-SemiBold.ttf"]
      }
     ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
