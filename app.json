{
  "expo": {
    "name": "Recoleto",
    "slug": "Recoleto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
    },
    "scheme": "recoleto",
    "plugins": [
     [
      "expo-font",
      {
        "fonts": ["./assets/fonts/Teachers-Regular.ttf", "./assets/fonts/Teachers-Bold.ttf", "./assets/fonts/Teachers-Medium.ttf", "./assets/fonts/Teachers-SemiBold.ttf"]
      }
     ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
